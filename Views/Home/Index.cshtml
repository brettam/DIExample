@model IEnumerable<string>

@{
    ViewBag.Title = "Home";
}
<h1>Hello to your Home</h1>
<h1>Cities</h1>
<ul class="list">
    @foreach (var city in Model)
    {
        <li>@city</li>
    }
</ul>